pages/main/modules_page.dart
----------------------------

ModulesPage
└── Scaffold
    ├── AppBar
    │   ├── Text ('Module')
    │   └── IconButton (Add)
    └── Body: Padding
        └── Conditional (_modules.isEmpty ? Center : ListView.builder)
            ├── Center
            │   └── Text ('Keine Module vorhanden')
            └── ListView.builder
                └── GestureDetector
                    └── Card
                        └── Column
                            ├── Row (Name, Code, LP)
                            ├── Text (Dozent, Raum, Kontakt)
                            ├── Divider
                            ├── Conditional (Klausuren)
                            └── Conditional (Noten)

