pages/main/week_plan_details_page.dart
--------------------------------------

WeekPlanDetailsPage
└── Scaffold
    ├── AppBar
    │   └── Titel: '${widget.day} - Details'
    └── Body
        └── Padding
            └── ListView
                └── Card (für jede Aufgabe)
                    └── ListTile
                        ├── Text (Modul, Doppelstunde, Raum)
                        ├── OnTap (zum Bearbeiten der Aufgabe)
                        └── Trailing
                            └── IconButton (zum Löschen der Aufgabe)

--------------------------------------
pages/sub/notifications/feed_page.dart
--------------------------------------

FeedPage
└── Scaffold
    ├── CustomAppBar
    │   ├── Titel: "RSS Feeds"
    │   ├── Leading Icon: Icons.arrow_back
    │   ├── On Leading Pressed: Navigator.pop(context)
    │   ├── Trailing Icon: Icons.edit
    │   └── On Trailing Pressed: _editFeeds
    └── Body
        └── Conditional (Nachricht und ListView)
            ├── Center
            │   └── Text ("Keine Feeds verfügbar")
            └── ListView.separated
                ├── itemCount: feeds.length
                ├── itemBuilder: Creates ListTile for each feed
                │   └── ListTile
                │       ├── title: feed.name
                │       ├── subtitle: feed.url
                │       └── onTap: _viewFeedDetail(feed)
                └── separatorBuilder: Divider

